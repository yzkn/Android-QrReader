<manifest xmlns:android="http://schemas.android.com/apk/res/android"
 package="jp.gr.java_conf.ya.qrreader" android:versionCode="3"
 android:versionName="0.14.0113">
 <supports-screens android:anyDensity="true"
  android:largeScreens="true"
  android:normalScreens="true"
  android:smallScreens="true" />
 <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="17" />
 <uses-permission android:name="android.permission.CAMERA" />
 <uses-feature android:name="android.hardware.camera" />
 <uses-feature android:name="android.hardware.camera.autofocus" android:required="false" />
 <uses-feature android:name="android.hardware.camera.flash" android:required="false" />
 <uses-feature android:name="android.hardware.camera.front" android:required="false" />
 <uses-feature android:name="android.hardware.camera.any" android:required="false" />
 <application android:icon="@drawable/ic_launcher"
  android:label="@string/app_name" android:theme="@style/AppTheme">
  <activity android:name=".QrReaderActivity" android:label="@string/title_activity_qr_reader"
   android:screenOrientation="landscape">
   <intent-filter>
    <action android:name="android.intent.action.MAIN" />
    <category android:name="android.intent.category.LAUNCHER" />
   </intent-filter>
  </activity>
  <activity android:name=".PrefActivity" android:label="@string/title_activity_qr_reader"
   android:screenOrientation="landscape" />
 </application>
